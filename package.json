{
	"name": "@johncision/rarity",
	"version": "3.3.0",
	"description": "",
	"scripts": {
		"lint": "node_modules/.bin/eslint --quiet ."
	},
	"eslintConfig": {
		"extends": [
			"@johncision/eslint-config/module"
		],
		"ignorePatterns": "spec/**",
		"env": {
			"node": false,
			"browser": true
		},
		"rules": {
			"class-methods-use-this": "warn",
			"no-empty-function": "warn",
			"fp/no-unused-expression": "off",
			"fp/no-nil": "off",
			"no-unused-vars": [
				"error",
				{
					"argsIgnorePattern": "^_",
					"varsIgnorePattern": "^_"
				}
			],
			"import/no-nodejs-modules": "error",
			"import/no-internal-modules": [
				"warn",
				{
					"allow": [
						"**/node_modules/@johncision/",
						"**/components/",
						"**/mutations/"
					]
				}
			],
			"spellcheck/spell-checker": [
				"error",
				{
					"minLength": 4,
					"skipWords": [
						"johncision",
						"applejacks",
						"namespace",
						"href",
						"importmap",
						"devtools",
						"lang",
						"influencers"
					]
				}
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johnCision/rarity.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/johnCision/rarity/issues"
	},
	"homepage": "https://github.com/johnCision/rarity#readme",
	"dependencies": {
		"@johncision/applejacks": "^3.5.0",
		"@johncision/applejacks-button": "^1.0.0",
		"@johncision/fluttershy": "^3.0.5",
		"@johncision/rarity-assets": "^9.2.1",
		"jwt-decode": "^3.1.2"
	},
	"devDependencies": {
		"@johncision/eslint-config": "^3.0.9",
		"eslint": "^8.0.1",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-immutable": "^1.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-no-loops": "^0.3.0",
		"eslint-plugin-security": "^1.4.0",
		"eslint-plugin-spellcheck": "^0.0.19"
	}
}
